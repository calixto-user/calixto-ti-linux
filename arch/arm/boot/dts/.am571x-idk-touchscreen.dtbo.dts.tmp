# 0 "arch/arm/boot/dts/am571x-idk-touchscreen.dtso"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm/boot/dts/am571x-idk-touchscreen.dtso"





/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 10 "arch/arm/boot/dts/am571x-idk-touchscreen.dtso" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/interrupt-controller/irq.h" 1
# 11 "arch/arm/boot/dts/am571x-idk-touchscreen.dtso" 2

&i2c1 {
 #address-cells = <1>;
 #size-cells = <0>;

 touchscreen: edt-ft5506@38 {
  compatible = "edt,edt-ft5506", "edt,edt-ft5x06";

  reg = <0x38>;

  interrupt-parent = <&gpio5>;
  interrupts = <6 2>;


  reset-gpios = <&gpio6 15 1>;

  touchscreen-size-x = <1920>;
  touchscreen-size-y = <1200>;

  wakeup-source;
 };
};
